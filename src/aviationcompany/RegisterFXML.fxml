<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="regPane" prefHeight="524.0" prefWidth="356.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aviationcompany.RegisterFXMLController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="524.0" prefWidth="356.0" spacing="20.0" style="-fx-background-color: #43494E;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Registration " textFill="WHITE">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <TextField fx:id="userName" promptText="Name" />
            <TextField fx:id="userID" promptText="Username" />
            <TextField fx:id="pass" promptText="Password" />
            <TextField fx:id="cpass" promptText="Confirm Password" />
            <TextField fx:id="email" promptText="Email" />
            <Label fx:id="error" textFill="WHITE" />
            <Button fx:id="register" mnemonicParsing="false" onAction="#registerAction" style="-fx-background-color: #E02840;" text="Register" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
